<%= render 'navigation' %>

<h1>New project</h1>

<%= form_for(@project, :url => admin_projects_path) do |form| %>

  <%= render :partial => 'form', :locals => { :f => form, :project => @project } %>

  <div class="actions">
    <%= form.submit 'Create' %>
    <%= link_to 'Back', admin_projects_path, :class => 'button' %>
  </div>
<% end %>

